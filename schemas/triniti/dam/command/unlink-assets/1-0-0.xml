<?xml version="1.0" encoding="UTF-8" ?>
<pbj-schema xmlns="http://gdbots.io/pbj/xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://gdbots.io/pbj/xsd http://gdbots.io/pbj/xsd/schema.xsd">

  <schema id="pbj:triniti:dam:command:unlink-assets:1-0-0">
    <fields>
      <field name="node_ref" type="node-ref" required="true"/>
      <field name="asset_refs" type="node-ref" rule="set">
        <description>
          A set of asset refs to unlink from this node ref.
        </description>
      </field>
    </fields>

    <mixins>
      <curie-major>gdbots:pbjx:mixin:command:v1</curie-major>
    </mixins>
  </schema>
</pbj-schema>
