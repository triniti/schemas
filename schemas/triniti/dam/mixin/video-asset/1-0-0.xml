<?xml version="1.0" encoding="UTF-8" ?>
<pbj-schema xmlns="http://gdbots.io/pbj/xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://gdbots.io/pbj/xsd http://gdbots.io/pbj/xsd/schema.xsd">

  <schema id="pbj:triniti:dam:mixin:video-asset:1-0-0" mixin="true" extends="pbj:triniti:dam:mixin:asset:1-0-0">
    <php-options>
      <insertion-points>
        <imports/>
        <methods>
          <![CDATA[
/**
 * @return array
 */
public function getUriTemplateVars()
{
    return ['_id' => (string)$this->get('_id')];
}
          ]]>
        </methods>
      </insertion-points>
    </php-options>

    <js-options>
      <insertion-points>
        <methods>
          <![CDATA[
/**
 * @returns {Object}
 */
getUriTemplateVars() {
  return { _id: `${this.get('_id', '')}` };
}
          ]]>
        </methods>
      </insertion-points>
    </js-options>
  </schema>
</pbj-schema>
