<?xml version="1.0" encoding="UTF-8" ?>
<pbj-schema xmlns="http://gdbots.io/pbj/xsd"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://gdbots.io/pbj/xsd http://gdbots.io/pbj/xsd/schema.xsd">

  <schema id="pbj:triniti:dam:mixin:image-asset:1-0-0" mixin="true" extends="pbj:triniti:dam:mixin:asset:1-0-0">
    <fields>
      <field name="width" type="medium-int">
        <description>
          The image width in pixels
        </description>
      </field>
      <field name="height" type="medium-int">
        <description>
          The image height in pixels
        </description>
      </field>
    </fields>
  </schema>
</pbj-schema>
