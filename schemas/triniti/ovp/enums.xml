      <option key="IAM_ROLE_ERROR_NOT_PERMITTED_OR_MALFORMED" value="1515"/>
      <option key="ACCELERATION_FAULT" value="1550"/>
      <option key="UNKNOWN_ERROR" value="1999"/>
      <option key="HTTP_FILE_ACCESS_NOT_AUTHORIZED" value="3401"/>
      <option key="HTTP_ACCESS_FORBIDDEN" value="3403"/>
      <option key="HTTP_FILE_NOT_FOUND" value="3404"/>
      <option key="HTTP_UPLOAD_ERROR" value="3408"/>
      <option key="HTTP_SERVER_ERROR" value="3450"/>
      <option key="HTTP_SERVER_CONNECTION_ERROR" value="3451"/>
      <option key="HTTP_UNKNOWN_ERROR" value="3999"/>
    </enum>
  </enums>
</pbj-enums>
