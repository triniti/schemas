<?xml version="1.0" encoding="UTF-8" ?>
<pbj-enums xmlns="http://gdbots.io/pbj/xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://gdbots.io/pbj/xsd http://gdbots.io/pbj/xsd/enums.xsd">

  <enums namespace="triniti:ovp">
    <enum name="search-videos-sort" type="string">
      <option key="UNKNOWN" value="unknown"/>
      <option key="RELEVANCE" value="relevance"/>
      <option key="POPULARITY" value="popularity"/>
      <option key="CREATED_AT_ASC" value="created-at-asc"/>
      <option key="CREATED_AT_DESC" value="created-at-desc"/>
      <option key="UPDATED_AT_ASC" value="updated-at-asc"/>
      <option key="UPDATED_AT_DESC" value="updated-at-desc"/>
      <option key="PUBLISHED_AT_ASC" value="published-at-asc"/>
      <option key="PUBLISHED_AT_DESC" value="published-at-desc"/>
      <option key="ORDER_DATE_ASC" value="order-date-asc"/>
      <option key="ORDER_DATE_DESC" value="order-date-desc"/>
      <option key="TITLE_ASC" value="title-asc"/>
      <option key="TITLE_DESC" value="title-desc"/>
    </enum>

    <!--
      @link https://en.wikipedia.org/wiki/TV_parental_guidelines_(US)
      @link https://developers.google.com/youtube/v3/docs/videos#contentDetails.contentRating.tvpgRating
    -->
    <enum name="tvpg-rating" type="string">
      <option key="UNKNOWN" value="unknown"/>
      <option key="TV_G" value="TV-G"/>
      <option key="TV_MA" value="TV-MA"/>
      <option key="TV_PG" value="TV-PG"/>
      <option key="TV_Y" value="TV-Y"/>
      <option key="TV_Y7" value="TV-Y7"/>
      <option key="TV_Y7_FV" value="TV-Y7-FV"/>
      <option key="TV_14" value="TV-14"/>
    </enum>

    <enum name="transcoding-status" type="string">
      <option key="UNKNOWN" value="unknown"/>
      <option key="PROCESSING" value="processing"/>
      <option key="CANCELED" value="canceled"/>
      <option key="COMPLETED" value="completed"/>
      <option key="FAILED" value="failed"/>
    </enum>

    <enum name="transcription-status" type="string">
      <option key="UNKNOWN" value="unknown"/>
      <option key="PROCESSING" value="processing"/>
      <option key="CANCELED" value="canceled"/>
      <option key="COMPLETED" value="completed"/>
      <option key="FAILED" value="failed"/>
    </enum>

    <enum name="mediaconvert-error-code" type="int">
      <option key="UNKNOWN" value="0"/>
      <option key="INPUT_ERROR" value="1010"/>
      <option key="VIDEO_ERROR" value="1020"/>
      <option key="AUDIO_ERROR" value="1021"/>
      <option key="UNSUPPORTED_CODEC" value="1030"/>
      <option key="SETTINGS_ERROR" value="1040"/>
      <option key="ACCELERATION_SETTINGS_ERROR" value="1041"/>
      <option key="JOB_DOESNT_REQUIRE_ENOUGH_PROCESSING_POWER_FOR_ACCELERATED_TRANSCODING" value="1042"/>
      <option key="DISK_FULL" value="1050"/>
      <option key="FILE_OPEN_ERROR" value="1056"/>
      <option key="CLIPPING_ERROR" value="1060"/>
      <option key="DEMUXER_PARSE_ERROR" value="1075"/>
      <option key="SOURCE_READ_ERROR" value="1076"/>
      <option key="MXF_OUTPUT_ERROR" value="1080"/>
      <option key="ENCRYPTION_ERROR" value="1091"/>
      <option key="PERMISSIONS_ISSUE" value="1401"/>
      <option key="FILE_OPEN_ERROR" value="1404"/>
      <option key="IAM_ROLE_ERROR_NOT_PERMITTED_OR_ANOTHER_PROBLEM" value="1432"/>
      <option key="IAM_ROLE_ERROR_DOESNT_EXIST" value="1433"/>
