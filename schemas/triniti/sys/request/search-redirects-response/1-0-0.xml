<?xml version="1.0" encoding="UTF-8" ?>
<pbj-schema xmlns="http://gdbots.io/pbj/xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://gdbots.io/pbj/xsd http://gdbots.io/pbj/xsd/schema.xsd">

  <schema id="pbj:triniti:sys:request:search-redirects-response:1-0-0">
    <fields>
      <field name="nodes" type="message" rule="list">
        <any-of>
          <curie>triniti:sys:mixin:redirect</curie>
        </any-of>
      </field>
    </fields>

    <mixins>
      <curie-major>gdbots:pbjx:mixin:response:v1</curie-major>
      <curie-major>gdbots:ncr:mixin:search-nodes-response:v1</curie-major>
    </mixins>
  </schema>
</pbj-schema>
