<?xml version="1.0" encoding="UTF-8" ?>
<pbj-schema xmlns="http://gdbots.io/pbj/xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://gdbots.io/pbj/xsd http://gdbots.io/pbj/xsd/schema.xsd">

  <schema id="pbj:triniti:taxonomy:request:suggest-hashtags-response:1-0-0">
    <fields>
      <field name="time_taken" type="medium-int">
        <description>How long in milliseconds it took to run the query.</description>
      </field>
      <field name="hashtags" type="string" format="hashtag" rule="list"/>
    </fields>

    <mixins>
      <curie-major>gdbots:pbjx:mixin:response:v1</curie-major>
    </mixins>
  </schema>
</pbj-schema>
