{
  "id": "http://schemas.triniti.io/json-schema/triniti/canvas/mixin/iframe-block/1-0-0.json#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "etag": {
      "type": "string",
      "pattern": "^[\\w\\.:-]+$",
      "pbj": {
        "type": "string",
        "rule": "single"
      }
    },
    "css_class": {
      "type": "string",
      "pattern": "^[\\w\\s-]+$",
      "description": "In rendering environments that support HTML the css_class can be appended to the dom elements' class attribute.",
      "pbj": {
        "type": "string",
        "rule": "single"
      }
    },
    "src": {
      "type": "string",
      "pattern": "^(https?:\\/\\/)?([\\da-z\\.-]+)\\.([a-z\\.]{2,6})([\\/\\w \\.-]*)*\\/?$",
      "pbj": {
        "type": "string",
        "rule": "single",
        "format": "url"
      }
    },
    "width": {
      "type": "string",
      "pattern": "^\\d+(%|px)?$",
      "pbj": {
        "type": "string",
        "rule": "single"
      }
    },
    "height": {
      "type": "string",
      "pattern": "^\\d+(%|px)?$",
      "pbj": {
        "type": "string",
        "rule": "single"
      }
    },
    "align": {
      "type": "string",
      "pattern": "^(center|right|left)$",
      "pbj": {
        "type": "string",
        "rule": "single"
      }
    }
  },
  "required": [
    "src"
  ],
  "additionalProperties": true
}