{
  "id": "http://schemas.triniti.io/json-schema/triniti/canvas/mixin/video-block/1-0-0.json#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "etag": {
      "type": "string",
      "pattern": "^[\\w\\.:-]+$",
      "pbj": {
        "type": "string",
        "rule": "single"
      }
    },
    "css_class": {
      "type": "string",
      "pattern": "^[\\w\\s-]+$",
      "description": "In rendering environments that support HTML the css_class can be appended to the dom elements' class attribute.",
      "pbj": {
        "type": "string",
        "rule": "single"
      }
    },
    "updated_date": {
      "type": "string",
      "format": "date-time",
      "description": "Used to visually represent an update that occurred on this block that is NOT when this block was literally last modified.",
      "pbj": {
        "type": "date-time",
        "rule": "single"
      }
    },
    "node_ref": {
      "type": "string",
      "pattern": "^[\\w\\/\\.:-]+$",
      "pbj": {
        "type": "identifier",
        "rule": "single"
      }
    },
    "autoplay": {
      "type": "boolean",
      "pbj": {
        "type": "boolean",
        "rule": "single"
      }
    },
    "muted": {
      "type": "boolean",
      "pbj": {
        "type": "boolean",
        "rule": "single"
      }
    },
    "start_at": {
      "type": "integer",
      "default": 0,
      "minimum": 0,
      "maximum": 65535,
      "pbj": {
        "type": "small-int",
        "rule": "single"
      }
    },
    "show_more_videos": {
      "type": "boolean",
      "pbj": {
        "type": "boolean",
        "rule": "single"
      }
    },
    "poster_image_ref": {
      "type": "string",
      "pattern": "^[\\w\\/\\.:-]+$",
      "description": "A reference to an image asset to use as the poster that will override what is provided by the target video.",
      "pbj": {
        "type": "identifier",
        "rule": "single"
      }
    }
  },
  "required": [
    "node_ref"
  ],
  "additionalProperties": true
}