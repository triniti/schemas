{
  "id": "http://schemas.triniti.io/json-schema/triniti/users/mixin/search-users-request/1-0-0.json#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "sort": {
      "type": "string",
      "default": "relevance",
      "enum": [
        "unknown",
        "relevance",
        "created-at-desc",
        "created-at-asc",
        "updated-at-desc",
        "updated-at-asc",
        "first-name-desc",
        "first-name-asc",
        "last-name-desc",
        "last-name-asc"
      ],
      "pbj": {
        "type": "string-enum",
        "rule": "single"
      }
    },
    "is_staff": {
      "type": "integer",
      "default": 0,
      "minimum": 0,
      "maximum": 2,
      "pbj": {
        "type": "trinary",
        "rule": "single"
      }
    },
    "is_blocked": {
      "type": "integer",
      "default": 2,
      "minimum": 0,
      "maximum": 2,
      "pbj": {
        "type": "trinary",
        "rule": "single"
      }
    }
  },
  "additionalProperties": true
}