{
  "id": "http://schemas.triniti.io/json-schema/triniti/notify/mixin/apple-news-notifier-result/1-0-0.json#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "apple_news_article_id": {
      "type": "string",
      "pattern": "^[a-z0-9-]+$",
      "pbj": {
        "type": "text",
        "rule": "single"
      }
    },
    "apple_news_share_url": {
      "type": "string",
      "pattern": "^(https?:\\/\\/)?([\\da-z\\.-]+)\\.([a-z\\.]{2,6})([\\/\\w \\.-]*)*\\/?$",
      "pbj": {
        "type": "text",
        "rule": "single",
        "format": "url"
      }
    },
    "apple_news_revision_token": {
      "type": "string",
      "pattern": "^[A-Za-z0-9=\\/]+$",
      "pbj": {
        "type": "text",
        "rule": "single"
      }
    }
  },
  "additionalProperties": true
}