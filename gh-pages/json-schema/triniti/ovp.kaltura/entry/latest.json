      "type": "string",
      "pattern": "^[\\w-]+$"
    },
    "redirect_entry_id": {
      "type": "string",
      "pattern": "^[\\w-]+$"
    },
    "root_entry_id": {
      "type": "string",
      "pattern": "^[\\w-]+$"
    },
    "parent_entry_id": {
      "type": "string",
      "pattern": "^[\\w-]+$"
    },
    "display_in_search": {
      "type": "string",
      "enum": [
        "unknown",
        -1,
        0,
        1,
        2
