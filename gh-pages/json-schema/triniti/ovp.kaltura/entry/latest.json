    "entry_id": {
      "type": "string",
      "pattern": "^[\\w-]+$"
    },
    "title": {
      "type": "string",
      "minLength": 0,
      "maxLength": 255
    },
    "description": {
      "type": "string",
      "minLength": 0,
      "maxLength": 65535
    },
    "partner_id": {
      "type": "string",
      "pattern": "^[\\w-]+$"
    },
    "user_id": {
      "type": "string",
      "minLength": 0,
      "maxLength": 255
    },
    "creator_id": {
      "type": "string",
      "minLength": 0,
      "maxLength": 255
