    "categories_ids": {
      "type": "array",
      "items": [
        {
          "type": "string",
          "pattern": "^[\\w-]+$"
        }
      ],
      "uniqueItems": true,
      "additionalProperties": false
    },
    "categories_normalized": {
      "type": "array",
      "items": [
        {
          "type": "string",
          "minLength": 0,
          "maxLength": 255
        }
