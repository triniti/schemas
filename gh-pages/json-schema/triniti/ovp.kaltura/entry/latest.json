    "views": {
      "type": "integer",
      "default": 0,
      "minimum": 0,
      "maximum": 4294967295
    },
    "last_played_at": {
      "type": "integer",
      "minimum": -2147483648,
      "maximum": 2147483647
    },
    "width": {
      "type": "integer",
      "default": 0,
      "minimum": 0,
      "maximum": 65535
    },
    "height": {
      "type": "integer",
      "default": 0,
      "minimum": 0,
      "maximum": 65535
