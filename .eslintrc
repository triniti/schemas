{
  "extends": "airbnb",
  "globals": {
    "document": true,
    "window": true
  },
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "rules": {
    "class-methods-use-this": "off",
    "comma-dangle": "off",
    "function-paren-newline": "off",
    "import/no-extraneous-dependencies": "off",
    "max-len": "off",
    "no-trailing-spaces": "off",
    "quotes": "off"
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
